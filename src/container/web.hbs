<div>
  <h1>Languages:</h1>
  <br> <hr>
  <div>
    {{#with web.languages}}
      <div>
        <ul>
          <div class="mb-5">
            {{#with [0]}}
              <h2><em>HTML</em></h2>
              <ul>
                <li class="mb-4">
                  <div> <abbr title="HTML">{{name}}</abbr></div>
                  <div>Version: <abbr title="Versia HTML">{{version}}</abbr></div>
                  <div>Description: {{description}}</div>
                </li>
                <hr>

                <div>
                  <h3><em>Elements:</em></h3>
                  {{#with elements}}
                    <div>
                      {{#with [0]}}
                        <ul>
                          <li class="mb-4">
                            <div>{{name}}</div>
                            <div>Description: {{description}}</div>
                          </li> <hr>

                          <div>
                            <h4><em>Attributes:</em></h4>
                            {{#with attributes}}
                              <div>
                                {{#with [0]}}
                                  <ul>
                                    <li class="mb-4">
                                      <div>{{name}}</div>
                                      <div>Description: {{description}}</div>
                                    </li>
                                  </ul>
                                {{/with}}
                              </div>

                              <div>
                                {{#with [1]}}
                                  <ul>
                                    <li>
                                      <div>{{name}}</div>
                                      <div>Description: {{description}}</div>
                                    </li>
                                  </ul>
                                {{/with}}
                              </div>
                            {{/with}}
                          </div>
                        </ul>
                      {{/with}}
                    </div>
                  {{/with}}
                </div>
              </ul>
            {{/with}}
          </div> <hr>

          <div>
            {{#with [1]}}
              <h2><em>CSS</em></h2>
              <ul>
                <li class="mb-4">
                  <div><abbr title="{{name}}">{{name}}</abbr></div>
                  <div>Version: {{version}}</div>
                  <div>Description: {{description}}</div>
                </li> <hr>

                <div>
                  <h3 class="mb-3"><em>Properties:</em></h3>
                  <div>
                    {{#with properties}}
                      <div>
                        {{#with [0]}}
                          <ul>
                            <li class="mb-4">
                              <div>{{name}}</div>
                              <div>Description: {{description}}</div>
                            </li> <hr>

                            <div>
                              <h4 class="mb-3"><em>Values:</em></h4>
                              {{#with values}}
                                <div class="mb-3">
                                  {{#with [0]}}
                                    <ul>
                                      <li>
                                        <div>{{value}}</div>
                                        <div>Description: {{description}}</div>
                                      </li>
                                    </ul>
                                  {{/with}}
                                </div>

                                <div>
                                  {{#with [1]}}
                                    <ul class="mb-4">
                                      <li>
                                        <div>{{value}}</div>
                                        <div>{{description}}</div>
                                      </li> 
                                    </ul>
                                    <hr>
                                  {{/with}}
                                </div>
                              {{/with}}
                            </div>
                          </ul> 
                        {{/with}}
                      </div>
                      
                      <div>
                        <h3 class="mb-3"><em>Background-color:</em></h3>
                        {{#with [1]}}
                          <ul>
                            <li class="mb-4">
                              <div>{{name}}</div>
                              <div>{{description}}</div>
                            </li> <hr>

                            <div>
                              <h4 class="mb-3"><em>Values:</em></h4>
                              {{#with values}}
                                <div class="mb-4">
                                  {{#with [0]}}
                                    <ul>
                                      <li>
                                        <div>{{value}}</div>
                                        <div>{{description}}</div>
                                      </li>
                                    </ul>
                                  {{/with}}
                                </div>

                                <div>
                                  {{#with [1]}}
                                    <ul>
                                      <li>
                                        <div>{{value}}</div>
                                        <div>{{description}}</div>
                                      </li>
                                    </ul>
                                  {{/with}}
                                </div>
                              {{/with}}
                            </div>
                          </ul>
                        {{/with}}
                      </div>
                    {{/with}}
                  </div>
                </div>
              </ul>
            {{/with}}
          </div> <br> <hr>

          <div>
            {{#with [2]}}
              <h2><em>JavaScript</em></h2>
              <ul>
                <li>
                  <div>{{name}}</div>
                  <div>Verdion: {{version}}</div>
                  <div>Description: {{description}}</div>
                </li> <br> <hr>

                <div>
                  <h3><em>Functions:</em></h3>
                  {{#with functions}}
                    <div>
                      {{#with [0]}}
                        <ul>
                          <li>
                            <div>{{name}}</div>
                            <div>{{description}}</div>
                          </li> <br> <hr>

                          <div>
                            <h4><em>Parameters:</em></h4>
                            {{#with parameters}}
                              <div>
                                {{#with [0]}}
                                  <ul>
                                    <li>
                                      <div>{{name}}</div>
                                      <div>{{type}}</div>
                                      <div>{{description}}</div>
                                    </li>
                                  </ul>
                                {{/with}}
                              </div>
                            {{/with}}
                          </div> <br> <hr>
                        </ul>
                      {{/with}}
                    </div>

                    <div>
                      <h4><em>GetElementById:</em></h4>
                      {{#with [1]}}
                        <ul>
                          <li>
                            <div>{{name}}</div>
                            <div>{{description}}</div>
                          </li> <br> <hr>

                          <div>
                            <h4><em>Parameters:</em></h4>
                            {{#with parameters}}
                              <div>
                                {{#with [0]}}
                                  <ul>
                                    <li>
                                      <div>{{name}}</div>
                                      <div>{{type}}</div>
                                      <div>{{description}}</div>
                                    </li>
                                  </ul>
                                {{/with}}
                              </div>
                            {{/with}}
                          </div>
                        </ul>
                      {{/with}}
                    </div>
                  {{/with}}
                </div>
              </ul>
            {{/with}}
          </div> <br> <hr>
        </ul>
      </div>
    {{/with}}
  </div>
</div>